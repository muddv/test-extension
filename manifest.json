{
    "manifest_version": 3,
    "version": "1.0",
    "name": "test",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting", 
        "activeTab"
    ],
    "content_scripts": [
    {   "matches": ["<all_urls>"],
        "js" : ["content.js"]}
    ]
}